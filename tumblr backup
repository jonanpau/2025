            <script>
                // Previous images
                const previousImages = [
                    'https://i.imgur.com/CAE1zYs.jpg',
                    'https://i.imgur.com/zQVkOq3.jpg',
                    'https://i.imgur.com/2CKCviq.jpg',
                    'https://i.imgur.com/BieOoRs.jpg',
                    'https://i.imgur.com/VdzM36u.jpg',
                    'https://i.imgur.com/cRShQer.jpg'
                ];

                // New images
                const newImages = [
                    'https://i.imgur.com/ZxAiu5b.jpg',
                    'https://i.imgur.com/67Tud7g.jpg',
                    'https://i.imgur.com/xMUz6GY.jpg',
                    'https://i.imgur.com/aVX5XXj.jpg',
                    'https://i.imgur.com/JzQcOjD.jpg',
                    'https://i.imgur.com/ajV1rZe.jpg'
                ];

                // Create full-width gallery
                const fullWidthGallery = document.querySelector('.full-width-gallery');
                
                previousImages.forEach(imageUrl => {
                    const galleryItem = document.createElement('div');
                    galleryItem.className = 'gallery-item';
                    
                    const img = document.createElement('img');
                    img.src = imageUrl;
                    img.alt = 'Nature Image';
                    
                    galleryItem.appendChild(img);
                    fullWidthGallery.appendChild(galleryItem);
                });

                // Create square gallery
                const squareGalleryContainer = document.createElement('div');
                squareGalleryContainer.className = 'square-gallery';
                fullWidthGallery.parentNode.insertBefore(squareGalleryContainer, fullWidthGallery.nextSibling);
                
                newImages.forEach(imageUrl => {
                    const galleryItem = document.createElement('div');
                    galleryItem.className = 'gallery-item';
                    
                    const img = document.createElement('img');
                    img.src = imageUrl;
                    img.alt = 'Nature Image';
                    
                    galleryItem.appendChild(img);
                    squareGalleryContainer.appendChild(galleryItem);
                });
            </script>
        </div>
    </div>


